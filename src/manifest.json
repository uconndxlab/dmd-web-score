{
  "name": "dmd-web-score",
  "description": "A chrome extension for scoring web pages",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "dmd-web-score",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ], 
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
